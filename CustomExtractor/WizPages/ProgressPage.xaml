<Page x:Class="CustomExtractor.WizPages.ProgressPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CustomExtractor.WizPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ProgressPage"
      Loaded="Page_Loaded">

    <Canvas>
        <TextBlock Name="StatusMessage" Canvas.Top="50" Canvas.Left="50"/>
        <ProgressBar Name="ProgressBar" Canvas.Top="75" Canvas.Left="50" Width="500" Height="25" Minimum="0" Maximum="100" Value="0"/>
    </Canvas>
</Page>
