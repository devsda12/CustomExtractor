<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CustomExtractor"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime">
    <!--All the global Strings are stored here-->
    <sys:String x:Key="App_Name">CustomExtractor</sys:String>
    <sys:String x:Key="Finish_Message">Thank you for using the CustomExtractor, all files have been extracted. You may close this window now.</sys:String>
</ResourceDictionary>